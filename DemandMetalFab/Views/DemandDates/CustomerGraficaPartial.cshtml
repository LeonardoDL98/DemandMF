

<script src="~/Scripts/chartjs-plugin-datalabels.js"></script>
<script src="~/Scripts/chartjs-plugin-datalabels.min.js"></script>

<h4 class="text-center">Stacked Bar Graph of Capacity by Customer </h4>
<br />
<canvas id="myChart"></canvas>

<div id="modalgrafica"></div>



<script>
   var data= JSON.parse('@(Html.Raw(ViewBag.JsonDataSet))');
    var config = {
  type: 'bar',
  data: data,
  options: {
    scales: {
      xAxes: [{
        stacked: true
      }],
      yAxes: [{
        stacked: true
      }]
    }
  }
};

var ctx = document.getElementById("myChart").getContext("2d");
    new Chart(ctx, config);

   
</script>
